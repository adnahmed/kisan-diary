{
  "name": "blues-stack-template",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle",
    "clean": "rimraf build public/build",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.js",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "pretest:e2e:run": "npm run build",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@chakra-ui/react": "^2.4.4",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@mui/x-data-grid": "^5.17.16",
    "@next-boost/hybrid-disk-cache": "^0.3.0",
    "@pankod/refine-core": "^3.90.6",
    "@pankod/refine-remix-router": "^1.7.0",
    "@pankod/refine-simple-rest": "^3.37.4",
    "@prisma/client": "^4.7.1",
    "@remix-run/express": "^1.8.2",
    "@remix-run/node": "^1.8.2",
    "@remix-run/react": "^1.8.2",
    "@remix-run/server-runtime": "^1.8.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "domain-functions": "^1.4.0",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-prometheus-middleware": "^1.2.0",
    "formik": "^2.2.9",
    "framer-motion": "^6",
    "isbot": "^3.6.3",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "prom-client": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-emojis": "^1.0.6",
    "react-hook-form": "^7.40.0",
    "react-select": "^5.7.0",
    "react-tabs": "^6.0.0",
    "remix-forms": "^1.3.0",
    "remix-image": "^1.4.0",
    "remix-routes": "^1.0.0",
    "remix-typedjson": "^0.1.6",
    "styled-components": "^5.3.6",
    "tiny-invariant": "^1.3.1",
    "yup": "^0.32.11",
    "zod": "^3.20.2",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@remix-run/dev": "^1.8.2",
    "@remix-run/eslint-config": "^1.8.2",
    "@remix-run/serve": "^1.9.0",
    "@testing-library/dom": "^8.19.0",
