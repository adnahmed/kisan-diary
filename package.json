{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:theme": "chakra-cli tokens ./app/styles/theme.ts",
    "build:server": "esbuild --platform=node --format=cjs ./expressServer.ts --outdir=build --bundle",
    "clean": "rimraf build public/build",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/expressServer.js",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "theme": "chakra-cli token ./app/styles/theme.ts",
    "test": "vitest",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "pretest:e2e:run": "npm run build",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.14",
    "@chakra-ui/react": "^2.4.4",
    "@chakra-ui/styled-system": "^2.5.0",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11",
    "@emotion/server": "^11",
    "@emotion/styled": "^11",
    "@fontsource/roboto": "^4.5.8",
    "@prisma/client": "^4.7.1",
    "@remix-run/express": "^1.9.0",
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@remix-run/server-runtime": "^1.9.0",
    "@remix-run/vercel": "^1.9.0",
    "@vercel/node": "^2.6.2",
    "bcryptjs": "^2.4.3",
    "chakra-react-select": "^4.4.3",
    "compression": "^1.7.4",
    "domain-functions": "^1.4.0",
    "express": "^4.18.2",
    "express-prometheus-middleware": "^1.2.0",
    "formik": "^2.2.9",
    "framer-motion": "^6",
    "isbot": "^3.6.5",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nanoid": "3.3.4",
    "prom-client": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-emojis": "^1.0.6",
    "react-hook-form": "^7.40.0",
    "react-icons": "^4.7.1",
    "react-select": "^5.7.0",
    "react-tabs": "^6.0.0",
    "remix-forms": "^1.3.0",
    "remix-routes": "^1.0.0",
    "remix-typedjson": "^0.1.6",
    "slugify": "^1.6.5",
    "tiny-invariant": "^1.3.1",
    "yup": "^0.32.11",
    "zod": "^3.20.2",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.2.1",
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.14",
    "@types/express-prometheus-middleware": "^1.2.1",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "^5.1.26",
    "chakra-cli": "^1.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.13",
    "eslint": "^8.27.0",
    "msw": "^0.49.2",
    "prisma": "^4.7.1",
    "remix-flat-routes": "^0.5.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.2.2",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}